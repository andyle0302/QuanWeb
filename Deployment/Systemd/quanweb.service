[Unit]
Description=QuanWeb
After=postgresql.service

[Service]
User=quan
Group=quan

Type=simple
WorkingDirectory=/home/quan/QuanWeb/quanweb
ExecStart=/home/quan/QuanWeb/venv/bin/gunicorn wsgi:app -b unix:../gunicorn.sock -k "egg:meinheld#gunicorn_worker"
TimeoutStopSec=20
KillMode=process
Restart=on-failure

[Install]
WantedBy=multi-user.target
